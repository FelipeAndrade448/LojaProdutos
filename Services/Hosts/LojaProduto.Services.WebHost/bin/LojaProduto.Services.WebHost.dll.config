<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
        <sectionGroup name="spring">
            <section name="context" type="Spring.Context.Support.ContextHandler, Spring.Core" />
            <section name="objects" type="Spring.Context.Support.DefaultSectionHandler, Spring.Core" />
            <section name="parsers" type="Spring.Context.Support.NamespaceParsersSectionHandler, Spring.Core" />
        </sectionGroup>
        <section name="syscache" type="NHibernate.Caches.SysCache.SysCacheSectionHandler, NHibernate.Caches.SysCache" />
        <sectionGroup name="common">
            <section name="logging" type="Common.Logging.ConfigurationSectionHandler, Common.Logging" />
        </sectionGroup>
        <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
        <sectionGroup name="antt.databases">
            <section name="LojaProduto" type="System.Configuration.NameValueSectionHandler" />
        </sectionGroup>
        <sectionGroup name="antt.servicos" />
    </configSections>
    <spring>
        <parsers>
            <parser type="Spring.Data.Config.DatabaseNamespaceParser, Spring.Data" />
            <parser type="Spring.Aop.Config.AopNamespaceParser, Spring.Aop" />
            <parser type="Spring.Transaction.Config.TxNamespaceParser, Spring.Data" />
        </parsers>
        <context>
            <resource uri="assembly://LojaProduto.Services.Impl/LojaProduto.Services.Impl.Config/Aspects.xml" />
            <resource uri="assembly://LojaProduto.Services.Impl/LojaProduto.Services.Impl.Config/Database.xml" />
            <resource uri="assembly://LojaProduto.Services.Impl/LojaProduto.Services.Impl.Config/Integration.xml" />
            <resource uri="assembly://LojaProduto.Services.Impl/LojaProduto.Services.Impl.Config/Services.xml" />
            <resource uri="~/Config/Log.xml" />
        </context>
    </spring>
    <syscache>
        <cache expiration="3600" priority="3" region="1hour" />
        <cache expiration="300" priority="3" region="5min" />
    </syscache>
    <common>
        <logging>
            <factoryAdapter type="Common.Logging.Log4Net.Log4NetLoggerFactoryAdapter, Common.Logging.Log4Net1213">
                <arg key="configType" value="FILE-WATCH" />
                <arg key="configFile" value="~/Config/Log.xml" />
            </factoryAdapter>
        </logging>
    </common>
    <antt.databases>
        <LojaProduto>
          <add key="db.datasource" value="SQEST407\SQLEXPRESS" />
          <add key="db.database" value="LojaProduto" />
          <add key="db.user" value="loja" />
          <add key="db.password" value="Washington@2017" />
        </LojaProduto>
    </antt.databases>
    <antt.servicos />
    <system.webServer>
        <modules>
            <remove name="WindowsAuthentication" />
            <remove name="PassportAuthentication" />
            <remove name="FormsAuthentication" />
            <remove name="FileAuthorization" />
        </modules>
    </system.webServer>
    <system.web>
        <compilation debug="true" targetFramework="4.5" />
    </system.web>
    <system.serviceModel>
        <bindings>
            <wsHttpBinding>
                <binding name="HTTP" closeTimeout="00:05:00" hostNameComparisonMode="StrongWildcard" maxBufferPoolSize="2147483647" maxReceivedMessageSize="2147483647" openTimeout="00:05:00" receiveTimeout="00:05:00" sendTimeout="00:05:00" transactionFlow="false">
                    <readerQuotas maxArrayLength="2147483647" maxBytesPerRead="2147483647" maxDepth="2147483647" maxNameTableCharCount="2147483647" maxStringContentLength="2147483647" />
                    <reliableSession enabled="false" inactivityTimeout="00:30:00" ordered="true" />
                    <security mode="None">
                        <transport clientCredentialType="None" />
                    </security>
                </binding>
            </wsHttpBinding>
        </bindings>
        <services>
            <service name="CadastroService" behaviorConfiguration="DefaultBehavior">
                <endpoint address="http://localhost/LojaProduto.Services.WebHost/Services/CadastroService.svc" binding="wsHttpBinding" bindingConfiguration="HTTP" contract="LojaProduto.Services.Spec.Services.ICadastroService" />
                <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
            </service>
        </services>
        <extensions>
            <behaviorExtensions>
                <add name="serviceVersionHelpPageBehavior" type="SQFramework.Net.Services.Behaviors.ServiceVersionHelpPageBehaviorExtension, SQFramework.Net" />
            </behaviorExtensions>
        </extensions>
        <behaviors>
            <serviceBehaviors>
                <behavior name="DefaultBehavior">
                    <serviceMetadata httpGetEnabled="true" />
                    <serviceDebug includeExceptionDetailInFaults="true" />
                    <serviceThrottling maxConcurrentCalls="150" maxConcurrentInstances="150" maxConcurrentSessions="150" />
                    <serviceVersionHelpPageBehavior assemblyName="LojaProduto.Services.Impl" />
                </behavior>
            </serviceBehaviors>
        </behaviors>
    </system.serviceModel>
    <runtime>
        <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
            <dependentAssembly>
                <assemblyIdentity name="Common.Logging" culture="neutral" publicKeyToken="af08829b84f0328e" />
